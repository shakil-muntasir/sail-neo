{
    "name": "ronvolt/laravel-sail-neo",
    "description": "Docker files for running a basic Laravel application.",
    "keywords": ["laravel", "docker"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/shakil-muntasir/sail-neo/issues",
        "source": "https://github.com/shakil-muntasir/sail-neo"
    },
    "authors": [
        {
            "name": "Muntasir Rahman",
            "email": "muntasir@ronvolt.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "bin": [
        "bin/sail"
    ],
    "autoload": {
        "psr-4": {
            "Ronvolt\\SailNeo\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Ronvolt\\SailNeo\\SailServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
